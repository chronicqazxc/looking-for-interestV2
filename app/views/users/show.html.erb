<div class="panel-heading"><h3><%= @user.name %></h3></div>
<div class="container-fluid">
	<table class="table">
  	<tr>
  		<th>Name</th>
		<td><%= @user.name %></td>
  	</tr>
  	<tr>
  		<th>Email</th>
  		<td><%= @user.email %></td>
  	</tr>
	</table>
</div>	
<div class="panel-footer">
  		<%= link_to "Back to List", users_path, :class => "btn btn-link" %> |
